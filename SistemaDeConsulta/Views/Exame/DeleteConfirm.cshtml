@using SistemaDeConsulta.Models.Entities;
@model Exame
@{
    ViewData["Title"] = "Deletar exame";
}

<div class="text-center">
    <h3>Realmente deseja deletar o exame (@Model.Nome)?</h3>
    <a role="button" class="btn btn-primary" asp-controller="Exame" asp-action="Index">Cancelar exclusão</a>
    <a role="button" class="btn btn-danger" asp-controller="Exame" asp-action="Delete" asp-route-id="@Model.Id">Confirma exclusão</a>
</div>

