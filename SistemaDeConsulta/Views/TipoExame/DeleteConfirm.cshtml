@using SistemaDeConsulta.Models.Entities;
@model TipoExame
@{
    ViewData["Title"] = "Deletar Tipo de exame";
}

<div class="text-center">
    <h3>Realmente deseja deletar o (@Model.Nome)?</h3>
    <a role="button" class="btn btn-primary" asp-controller="TipoExame" asp-action="Index">Cancelar exclusão</a>
    <a role="button" class="btn btn-danger" asp-controller="TipoExame" asp-action="Delete" asp-route-id="@Model.Id">Confirma exclusão</a>
</div>
